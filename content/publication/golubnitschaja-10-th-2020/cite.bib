 issn = {1878-5077},
 journal = {EPMA J},
 month = {August},
 pages = {1--133},
 pmcid = {PMC7435254},
 pmid = {32837665},
 title = {10th Anniversary of the European Association for Predictive, Preventive and Personalised (3P) Medicine - EPMA World Congress Supplement 2020},
 url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC7435254/},
 urldate = {2021-03-24},
